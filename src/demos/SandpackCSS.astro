<style is:global lang="scss">
  .sp-wrapper.sandpack-wrapper {
    --sp-layout-height: 500px;
    --sp-space-1: 4px;
    --sp-space-2: 8px;
    --sp-space-3: 12px;
    --sp-space-4: 16px;
    --sp-space-5: 20px;
    --sp-space-6: 24px;
    --sp-space-7: 28px;
    --sp-space-8: 32px;
    --sp-space-9: 36px;
    --sp-space-10: 40px;
    --sp-space-11: 44px;
    --sp-border-radius: 8px;
    --sp-layout-headerHeight: 40px;
    --sp-transitions-default: 150ms ease;
    --sp-zIndices-base: 1;
    --sp-zIndices-overlay: 2;
    --sp-zIndices-top: 3;
    --sp-colors-surface1: #ffffff;
    --sp-colors-surface2: #efefef;
    --sp-colors-surface3: #f3f3f3;
    --sp-colors-disabled: #c5c5c5;
    --sp-colors-base: #323232;
    --sp-colors-clickable: #808080;
    --sp-colors-hover: #4d4d4d;
    --sp-colors-accent: #59873a;
    --sp-colors-error: #ea3323;
    --sp-colors-errorSurface: #fcf1f0;
    --sp-colors-warning: #6a4516;
    --sp-colors-warningSurface: #fef2c0;
    --sp-font-body: 'IBM Plex Mono', Menlo, 'Noto Sans Mono', 'Fira Code',
      'Fira Mono', ui-monospace, SFMono-Regular, 'SF Mono', Monaco, Consolas,
      'Liberation Mono', 'Courier New', monospace;
    --sp-font-mono: ;
    --sp-font-size: 13px;
    --sp-font-lineHeight: 20px;
    --sp-syntax-color-plain: #393a34;
    --sp-syntax-color-comment: #a0ada0;
    --sp-syntax-color-keyword: #ab5959;
    --sp-syntax-color-tag: #1e754f;
    --sp-syntax-color-punctuation: #999999;
    --sp-syntax-color-definition: #59873a;
    --sp-syntax-color-property: #b07d48;
    --sp-syntax-color-static: #a65e2b;
    --sp-syntax-color-string: #b56959;
  }

  .dark .sp-wrapper.sandpack-wrapper {
    --sp-colors-surface1: #121212;
    --sp-colors-surface2: #222222;
    --sp-colors-surface3: #3e3e3e;
    --sp-colors-disabled: #c5c5c5;
    --sp-colors-base: #323232;
    --sp-colors-clickable: #808080;
    --sp-colors-hover: #4d4d4d;
    --sp-colors-accent: #59873a;
    --sp-colors-error: #ea3323;
    --sp-colors-errorSurface: #fcf1f0;
    --sp-colors-warning: #6a4516;
    --sp-colors-warningSurface: #fef2c0;
    --sp-syntax-color-plain: #dbd7caee;
    --sp-syntax-color-comment: #758575dd;
    --sp-syntax-color-keyword: #cb7676;
    --sp-syntax-color-tag: #4d9375;
    --sp-syntax-color-punctuation: #666666;
    --sp-syntax-color-definition: #80a665;
    --sp-syntax-color-property: #bd976a;
    --sp-syntax-color-static: #bd976a;
    --sp-syntax-color-string: #c98a7d;
  }

  .sp-wrapper .sp-pre-placeholder {
    font-size: var(--sp-font-size);
    margin: 0;
    flex-grow: 2;
    flex-shrink: 0;
    display: block;
    white-space: pre;
    word-wrap: normal;
    box-sizing: border-box;
    min-height: 100%;
    padding: var(--sp-space-4) 0;
    outline: none;
    overflow: initial;
    line-height: var(--sp-font-lineHeight);
    margin-left: var(--sp-space-5) !important;
  }

  @keyframes sp-k-eyOShd {
    0% {
      opacity: 0;
    }

    100% {
      opacity: 1;
    }
  }

  @keyframes sp-k-iOHdLQ {
    0% {
      transform: rotateX(-25.5deg) rotateY(45deg);
    }

    100% {
      transform: rotateX(-25.5deg) rotateY(405deg);
    }
  }

  .sp-button svg {
    margin: auto;
  }

  .sp-button,
  .sp-tab-button {
    -webkit-appearance: none;
    appearance: none;
    outline: none;
    display: flex;
    align-items: center;
    font-size: inherit;
    font-family: inherit;
    background-color: transparent;
    transition:
      color var(--sp-transitions-default),
      background var(--sp-transitions-default);
    cursor: pointer;
    color: var(--sp-colors-clickable);
    border: 0;
    text-decoration: none;
  }

  .sp-button:disabled,
  .sp-tab-button:disabled {
    color: var(--sp-colors-disabled);
  }

  .sp-button:hover:not(:disabled, [data-active='true']),
  .sp-tab-button:hover:not(:disabled, [data-active='true']) {
    color: var(--sp-colors-hover);
  }

  .sp-button[data-active='true'],
  .sp-tab-button[data-active='true'] {
    color: var(--sp-colors-accent);
  }

  .sp-button svg,
  .sp-tab-button svg {
    min-width: var(--sp-space-4);
    width: var(--sp-space-4);
    height: var(--sp-space-4);
  }

  .sp-button {
    padding: var(--sp-space-1);
    height: var(--sp-space-7);
    display: flex;
  }

  .sp-button:not(:has(span)) {
    width: var(--sp-space-7);
  }

  .sp-button:has(svg + span) {
    padding-right: var(--sp-space-3);
    padding-left: var(--sp-space-2);
    gap: var(--sp-space-1);
  }

