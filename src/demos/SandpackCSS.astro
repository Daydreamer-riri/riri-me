  .sp-stack {
    display: flex;
    flex-direction: column;
    width: 100%;
    position: relative;
    background-color: var(--sp-colors-surface1);
    gap: 1px;
  }

  .sp-stack:has(.sp-stack) {
    background-color: var(--sp-colors-surface2);
  }

  .sp-cube {
    transform: translate(-4px, 9px) scale(0.13, 0.13);
  }

  .sp-cube * {
    position: absolute;
    width: 96px;
    height: 96px;
  }

  .sp-wrapper {
    all: initial;
    font-size: var(--sp-font-size);
    font-family: var(--sp-font-body);
    display: block;
    box-sizing: border-box;
    text-rendering: optimizeLegibility;
    -webkit-tap-highlight-color: transparent;
    -webkit-font-smoothing: subpixel-antialiased;
  }

  @media screen and (min-resolution: 2dppx) {
    .sp-wrapper {
      -webkit-font-smoothing: antialiased;
      -moz-osx-font-smoothing: grayscale;
    }
  }

  .sp-wrapper * {
    box-sizing: border-box;
  }

  .sp-wrapper .sp-wrapper:focus {
    outline: 0;
  }

  .sp-layout {
    border: 1px solid var(--sp-colors-surface2);
    display: flex;
    flex-wrap: wrap;
    align-items: stretch;
    border-radius: var(--sp-border-radius);
    overflow: hidden;
    position: relative;
    background-color: var(--sp-colors-surface2);
    gap: 1px;
  }

  .sp-layout > .sp-stack {
    flex-grow: 1;
    flex-shrink: 1;
    flex-basis: 0;
    height: var(--sp-layout-height);
    overflow: hidden;
  }

  @media print {
    .sp-layout > .sp-stack {
      height: auto;
      display: block;
    }
  }

  @media screen and (max-width: 768px) {
    .sp-layout > .sp-stack:not(.sp-preview, .sp-editor, .sp-preset-column) {
      height: calc(var(--sp-layout-height) / 2);
    }
  }

  @media screen and (max-width: 768px) {
    .sp-layout > .sp-stack {
      min-width: 100%;
    }
  }

  .sp-layout > .sp-file-explorer {
    flex: 0.2;
    min-width: 200px;
  }

  @media screen and (max-width: 768px) {
    .sp-layout > .sp-file-explorer {
      flex: 1;
    }
  }

  .sp-code-editor {
    flex: 1;
    position: relative;
    overflow: auto;
    background: var(--sp-colors-surface1);
  }

  .sp-code-editor .cm-scroller {
    padding: var(--sp-space-4) 0;
    line-height: var(--sp-font-lineHeight);
  }

  @media screen and (max-width: 768px) {
    @supports (-webkit-overflow-scrolling: touch) {
      .sp-code-editor .cm-content {
        font-size: 16px;
      }
    }
  }

  .sp-wrapper .cm-content {
    margin: 0;
    flex-grow: 2;
    flex-shrink: 0;
    display: block;
    white-space: pre;
    word-wrap: normal;
    box-sizing: border-box;
    min-height: 100%;
    padding: 0 var(--sp-space-4);
    outline: none;

    .cm-line {
      padding-left: var(--sp-space-1);
    }
  }

  .sp-tabs {
    border-bottom: 1px solid var(--sp-colors-surface2);
    background: var(--sp-colors-surface1);
  }

  .sp-tabs-scrollable-container {
    padding: 0 var(--sp-space-2);
    overflow: auto;
    display: flex;
    flex-wrap: nowrap;
    align-items: stretch;
    min-height: 40px;
    margin-bottom: -1px;
  }

  .sp-tab-button {
    padding: 0 var(--sp-space-2);
    height: var(--sp-layout-headerHeight);
    white-space: nowrap;
  }

  .sp-tab-button:focus {
    outline: none;
  }

  .sp-tab-button:hover > .sp-c-hfoyCM {
    visibility: unset;
  }

  .sp-cm {
    margin: 0;
    outline: none;
    height: 100%;
  }

  .sp-cm .sp-syntax-string {
    color: var(--sp-syntax-color-string);
    font-style: var(--sp-syntax-fontStyle-string);
  }

  .sp-cm .sp-syntax-plain {
    color: var(--sp-syntax-color-plain);
    font-style: var(--sp-syntax-fontStyle-plain);
  }

  .sp-cm .sp-syntax-comment {
    color: var(--sp-syntax-color-comment);
    font-style: var(--sp-syntax-fontStyle-comment);
  }

  .sp-cm .sp-syntax-keyword {
    color: var(--sp-syntax-color-keyword);
    font-style: var(--sp-syntax-fontStyle-keyword);
  }

  .sp-cm .sp-syntax-definition {
    color: var(--sp-syntax-color-definition);
    font-style: var(--sp-syntax-fontStyle-definition);
  }

  .sp-cm .sp-syntax-punctuation {
    color: var(--sp-syntax-color-punctuation);
    font-style: var(--sp-syntax-fontStyle-punctuation);
  }

  .sp-cm .sp-syntax-property {
    color: var(--sp-syntax-color-property);
    font-style: var(--sp-syntax-fontStyle-property);
  }

  .sp-cm .sp-syntax-tag {
    color: var(--sp-syntax-color-tag);
    font-style: var(--sp-syntax-fontStyle-tag);
  }

  .sp-cm .sp-syntax-static {
    color: var(--sp-syntax-color-static);
    font-style: var(--sp-syntax-fontStyle-static);
  }

  .sp-preview-container {
    flex: 1;
    display: flex;
    flex-direction: column;
    background: white;
    overflow: auto;
    position: relative;
  }

  .sp-preview-container .sp-bridge-frame {
    border: 0;
    position: absolute;
    left: var(--sp-space-2);
    bottom: var(--sp-space-2);
    z-index: var(--sp-zIndices-top);
    height: 12px;
    width: 30%;
    mix-blend-mode: multiply;
    pointer-events: none;
  }

  .sp-preview-iframe {
    border: 0;
    outline: 0;
    width: 100%;
    height: 100%;
    min-height: 160px;
    max-height: 2000px;
    flex: 1;
  }

  .sp-preview-actions {
    display: flex;
    position: absolute;
    bottom: var(--sp-space-2);
    right: var(--sp-space-2);
    z-index: var(--sp-zIndices-overlay);
    gap: var(--sp-space-2);
  }

  .sp-button {
    background-color: var(--sp-colors-surface2);
    border-radius: 99999px;
    border: 1px solid var(--sp-colors-surface3);
  }

  .sp-button[data-active='true'] {
    color: var(--sp-colors-surface1);
    background: var(--sp-colors-accent);
  }

  .sp-button:hover:not(:disabled, [data-active='true']) {
    background-color: var(--sp-colors-surface3);
  }

  --sxs {
    --sxs: 3 sp-c-PJLV-kCOVwI-status-pass sp-c-PJLV-kEzYsr-status-fail
      sp-c-PJLV-gHAhSA-status-skip sp-c-PJLV-jgnHyR-status-title
      sp-c-PJLV-iCgxLS-status-run sp-c-PJLV-bnDZSy-status-pass
      sp-c-PJLV-eYuGwt-status-fail sp-wrapper-gMQIch-variant-light;
  }

  .sp-bridge-frame {
    display: none;
  }

